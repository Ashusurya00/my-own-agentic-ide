{
    "name": "agentic-ide",
    "displayName": "Agentic IDE",
    "description": "AI powered agentic IDE",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:agentic-ide.refactorFile",
        "onCommand:agentic-ide.generateFiles"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "agentic-ide.refactorFile",
                "title": "AI: Refactor Current File"
            },
            {
                "command": "agentic-ide.generateFiles",
                "title": "AI: Generate Frontend Files"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.85.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "axios": "^1.6.0"
    }
}